<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ("partials/header") %>
    <title>Post</title>
</head>

<body>
  <%- include ("partials/nav") %>
    <main style="padding: 3rem">
      <%- include ("partials/message") %>
        <form action="/profile/post" method="POST">
          <div class="form-group">
            <label for="exampleforTitle">Title:</label>
            <input name="title" type="text" class="form-control" id="exampleforTitle" />
          </div>
          <br />
          <div class="form-group">
            <label for="exampleforContent">Content:</label>
            <textarea class="form-control" id="exampleforContent" aria-describedby="emailHelp"
              name="content" /></textarea>
            <br />
            <button type="submit" class="btn btn-primary">Submot</button>
        </form>
    </main>
</body>

</html>